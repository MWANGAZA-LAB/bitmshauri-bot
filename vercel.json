{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python@3.0.0"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
