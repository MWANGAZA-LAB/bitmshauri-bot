{
  "version": 2,
  "functions": {
    "api/bot.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
