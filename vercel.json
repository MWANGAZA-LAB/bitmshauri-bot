{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "api/bot.py": {
      "maxDuration": 30
    }
  }
}
